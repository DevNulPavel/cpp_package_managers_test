# https://github.com/theodelrieu/conan-darwin-toolchain
# TODO: Плохо собирается со старыми векрсиями iOS

include(default)

[settings]
os=iOS
os.version=10.0
arch=armv7
build_type=Release

[build_requires]
darwin-toolchain/1.0.8@theodelrieu/stable

[options]
darwin-toolchain:bitcode=True
libcurl:darwin_ssl=False